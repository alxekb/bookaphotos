!!!
%html
  %head
    %title bookaphotos.com
    = csrf_meta_tags
    = stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload'
    = javascript_include_tag 'application', 'data-turbolinks-track': 'reload'
  %body
    - if notice
      %p.notice= notice
    - if alert
      %p.alert= alert
    = render 'header'
    %main.container{ role: "main" }
      = yield
